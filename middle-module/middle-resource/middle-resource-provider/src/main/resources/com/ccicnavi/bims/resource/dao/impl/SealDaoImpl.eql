-- [listSeal]
SELECT
    sel_uuid as selUuid, sel_name as selName, sel_eng_name as selEngName, sel_cert_name as selCertName, sel_enm_uuid as selEnmUuid,
    sel_count as selCount, sel_batch_no as selBatchNo, sel_start_number as selStartNumber, sel_end_number as selEndNumber,
    sel_person as selPerson, sel_keeper as selKeeper, is_deleted as isDeleted, created_time as createdTime,created_uuid as createdUuid,
    updated_time as updatedTime, updated_name as updatedName, business_line as businessLine, org_uuid as orgUuid, app_sys_uuid as appSysUuid,
    extend1 as extend1, extend2 as extend2, extend3 as extend3
FROM
    res_seal
WHERE
    is_deleted = 'N'
      --iff selName != null
      and sel_name like '#selName:like#'

      --iff selEngName != null
      and sel_eng_name like '#selEngName:leftLike#'

      --iff selCertName != null
      and sel_cert_name like '#selCertName:rightLike#'

      --iff selEnmUuid != null
      and sel_enm_uuid = '#selEnmUuid#'

      --iff selCount != null
      and sel_count = '#selCount#'

      --iff selBatchNo != null
      and sel_batch_no like '#selBatchNo:like#'

      --iff selStartNumber != null
      and sel_start_number like '#selStartNumber:like#'

      --iff selEndNumber != null
      and sel_end_number = '#selEndNumber#'

      --iff selPerson != null
      and sel_person = '#selPerson#'

      --iff selKeeper != null
      and sel_keeper = '#selKeeper#'

      --iff isDeleted != null
      and is_deleted = '#isDeleted#'

      --iff createdTime != null
      and created_time = '#createdTime#'

      --iff createdUuid != null
      and created_uuid = '#createdUuid#'

      --iff updatedTime != null
      and updated_time = '#updatedTime#'

      --iff updatedName != null
      and updated_name = '#updatedName#'

      --iff businessLine != null
      and business_line = '#businessLine#'

      --iff orgUuid != null
      and org_uuid = '#orgUuid#'

      --iff appSysUuid != null
      and app_sys_uuid = '#appSysUuid#'

      --iff extend1 != null
      and extend1 = '#extend1#'

      --iff extend2 != null
      and extend2 = '#extend2#'

      --iff extend3 != null
      and extend3 = '#extend3#'

      --iff orderBy == null or orderBy == ''
      ORDER BY created_time DESC

      --iff orderBy != null and orderBy != ''
      ORDER BY '#orderBy#'


