        res_seal
     -- trim prefix= set suffixOverrides=,
            is_deleted = 'Y'
      -- end
      where
            --iff selUuid != null and  selUuid != ''
                sel_uuid = '#selUuid#'
            --end

            --iff uuids != null
                sel_uuid  in
                -- for item=item index=index collection=uuids open=( separator=, close=)
                    '#item#'
                -- end
            --end



