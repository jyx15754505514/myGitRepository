        --iff splPurposeQty != null and splPurposeQty != ''
            '#splPurposeQty#',
    -- end

-- [updateOrderSampleType]
UPDATE
    order_sample_type
    -- trim prefix= set suffixOverrides=,
        --iff orderSplUuid != null and orderSplUuid != ''
            order_spl_uuid = '#orderSplUuid#',
        --iff orderUuid != null and orderUuid != ''
            order_uuid = '#orderUuid#',
        --iff splPurposeType != null and splPurposeType != ''
            spl_purpose_type = '#splPurposeType#',
        --iff splPurposeQty != null and splPurposeQty != ''
            spl_purpose_qty = '#splPurposeQty#',
    -- end
where
    --iff orderSplUuid != null and orderSplUuid != ''
         order_spl_uuid = '#orderSplUuid#'
    --iff orderSplUuids != null
        and order_spl_uuid in
        -- for item=item index=index collection=orderSplUuids open=( separator=, close=)
            '#item#'
        -- end

