-- [listOrderSampleType]
SELECT
    order_spl_uuid orderSplUuid,
    order_uuid orderUuid,
    spl_purpose_type splPurposeType,
    spl_purpose_qty splPurposeQty
from
    order_sample_type
where
--iff orderUuid != null and orderUuid != ''
     order_uuid =  '#orderUuid#'
--end

