-- [listShipment]
    order_uuid orderUuid,
    shipping_type_id shippingTypeId,
    ship_name shipName,
    voyage_name voyageName,
    dispath_port_id dispathPortId,
    dest_port_id destPortId,
    loading_port_id loadingPortId,
    discharge_port_id dischargePortId,
    trade_nature_id tradeNatureId,
    customs_cert_no customsCertNo,
    customs_decl_no customsDeclNo,
    inspection_person inspectionPerson,
    weight weight,
    weight_unit weightUnit
from
    order_shipment
where
    is_deleted = 'N'

-- [getShipment]
    order_uuid orderUuid,
    shipping_type_id shippingTypeId,
    ship_name shipName,
    voyage_name voyageName,
    dispath_port_id dispathPortId,
    dest_port_id destPortId,
    loading_port_id loadingPortId,
    discharge_port_id dischargePortId,
    trade_nature_id tradeNatureId,
    customs_cert_no customsCertNo,
    customs_decl_no customsDeclNo,
    inspection_person inspectionPerson,
    weight weight,
    weight_unit weightUnit
from
    order_shipment
where
    is_deleted = 'N'
--iff orderUuid != null and orderUuid != ''
    and order_uuid = '#orderUuid#'
--iff orderUuidList != null
and order_uuid in
-- for item=item index=index collection=orderUuidList open=( separator=, close=)
    '#item#'
-- end

