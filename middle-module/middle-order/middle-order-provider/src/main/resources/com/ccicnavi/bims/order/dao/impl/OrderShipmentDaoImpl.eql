-- [updateOrderShipment]
    update order_shipment
    -- trim prefix= set suffixOverrides=,
      --iff orderUuid != null
        order_uuid = '#orderUuid#',
      --iff shippingTypeId != null
        shipping_type_id = '#shippingTypeId#',
      --iff shipName != null
        ship_name = '#shipName#',
      --iff voyageName != null
        voyage_name = '#voyageName#',
      --iff dispathPortId != null
        dispath_port_id = '#dispathPortId#',
      --iff destPortId != null
        dest_port_id = '#destPortId#',,
      --iff loadingPortId != null
        loading_port_id = '#loadingPortId#',
      --iff dischargePortId != null
        discharge_port_id = '#dischargePortId#',
      --iff tradeNatureId != null
        trade_nature_id = '#tradeNatureId#',
      --iff customsCertNo != null
        customs_cert_no = '#customsCertNo#',
      --iff customsDeclNo != null
        customs_decl_no = '#customsDeclNo#',
      --iff inspectionPerson != null
        inspection_person = '#inspectionPerson#',
      --iff weight != null
        weight = '#weight#',
      --iff weightUnit != null
        weight_unit = '#weightUnit#'
    -- end
where
    is_deleted = 'N'
