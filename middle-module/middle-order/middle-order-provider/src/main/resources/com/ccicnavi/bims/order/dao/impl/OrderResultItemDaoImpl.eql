-- [listOrderResultItem]
SELECT
    result_item_uuid resultItemUuid,
        result_uuid resultUuid,
        order_uuid orderUuid,
        order_item_uuid orderItemUuid
from
    order_result_item
where
   1=1
--iff resultItemUuid != null and resultItemUuid != ''
    and result_item_uuid = '#resultItemUuid#'
    --iff resultUuid != null and resultUuid != ''
        and result_uuid = '#resultUuid#'
--end

