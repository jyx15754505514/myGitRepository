-- [updateOrderResultInfo]
 update order_result_info
    -- trim prefix=SET suffixOverrides=,
                 -- iff resultNo != null
                        result_no = '#resultNo#',
                      -- iff certPaperNo != null
                        cert_paper_no = '#certPaperNo#',
                      -- iff certEditerUuid != null
