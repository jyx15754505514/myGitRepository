-- [deleteOrderResultInfo]
UPDATE order_result_info
SET is_deleted = 'Y'
WHERE
	is_deleted = 'N'
	--iff resultUuid != null and resultUuid != ''
        AND      result_uuid = '#resultUuid#'
 -- if resultUuids != null
            AND result_uuid IN
            -- for item=item index=index collection=resultUuids open=( separator=, close=)
                '#item#'
-- end
