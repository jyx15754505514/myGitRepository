-- [updateLogDetail]
UPDATE
    sys_log_detail
-- trim prefix= set suffixOverrides=,
    --ifflogUuid != null and ifflogUuid != ''
        log_uuid = '#logUuid#'
    --iffoperName != null and iffoperName != ''
        oper_name = '#operName#'
    --iffoperTime != null
        oper_time = '#operTime#'
    --iffoperContent != null and iffoperContent != ''
        oper_content = '#operContent#'
    --iffoperModule != null and iffoperModule != ''
        oper_module = '#operModule#'
    --iffcomments != null and iffcomments != ''
        comments = '#comments#'
    --ifforgUuid != null and ifforgUuid != ''
        org_uuid = '#orgUuid#'
    --iffappSysUuid != null and iffappSysUuid != ''
        app_sys_uuid = '#appSysUuid#'
    --iffbusinessLine != null and iffbusinessLine != ''
        business_line = '#businessLine#'
-- end
where
    is_deleted = 'N'
    --iff logUuid != null and logUuid != ''
        and log_uuid = '#logUuid#'
    --iff logUuids != null
        and log_uuid in
        -- for item=item index=index collection=logUuids open=( separator=, close=)
            '#item#'
        -- end

