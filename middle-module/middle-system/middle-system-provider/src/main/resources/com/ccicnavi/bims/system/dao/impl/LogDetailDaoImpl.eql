-- [getLogDetail]
SELECT
    log.log_uuid logUuid,
    log.oper_name operName,
    log.oper_time operTime,
    log.oper_content operContent,
    log.oper_module operModule,
    log.comments comments,
    log.org_uuid orgUuid,
    log.app_sys_uuid appSysUuid,
    log.business_line businessLine
from
    sys_log_detail as log
where
    log.is_deleted = 'N'
    --iff logUuid != null and logUuid != ''
        and log.log_uuid = '#logUuid#'
    --end

