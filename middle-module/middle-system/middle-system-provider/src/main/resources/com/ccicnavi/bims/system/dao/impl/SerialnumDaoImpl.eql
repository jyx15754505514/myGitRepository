-- [getSerialnumCfg]
SELECT
snc_uuid sncUuid,
snc_name sncName,
snc_init_value sncInitValue,
snc_length sncLength,
snc_step sncStep,
snc_period sncPeriod,
dis_dept disDept,
sort_num sortNum,
comments comments,
is_deleted isDeleted,
org_uuid orgUuid,
app_sys_uuid appSysUuid,
prod_catalog_uuid prodCatalogUuid,
extend1 extend1,
extend2 extend2,
extend3 extend3
from sys_serialnum_cfg
WHERE snc_uuid = '#sncUuid#' and is_deleted = 'N'
-- if orgUuid != null and orgUuid != ''
  and org_uuid = '#orgUuid#'
-- end
-- if appSysUuid != null and appSysUuid != ''
  and app_sys_uuid = '#appSysUuid#'
-- end
-- if prodCatalogUuid != null and prodCatalogUuid != ''
  and prod_catalog_uuid = '#prodCatalogUuid#'
-- end

