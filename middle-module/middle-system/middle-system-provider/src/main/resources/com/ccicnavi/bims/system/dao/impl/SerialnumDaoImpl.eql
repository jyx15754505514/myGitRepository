-- [getSerialnumCfgItem]
    select
    sncd_uuid sncdUuid,
    snc_uuid sncUuid,
    sncd_type sncdType,
    sncd_value sncdValue,
    sncd_order_num sncdOrderNum
    from sys_serialnum_cfg_item
    where sncd_uuid='#sncdUuid#'


-- [insertSerialnumCfgItem]
    INSERT into sys_serialnum_cfg_item
    (sncd_uuid,
      snc_uuid,
      sncd_type,
      sncd_value,
      sncd_order_num) values
    ('#sncdUuid#',
                  '#sncUuid#',
                  '#sncdType#',
                  '#sncdValue#',
                  '#sncdOrderNum#')

-- [deleteSerialnumCfgItem]
    delete from
        sys_serialnum_cfg_item
    WHERE snc_uuid = '##'

