-- [deleteSubLinkman]
UPDATE crm_subc_linkman
SET is_deleted='Y'
WHERE
    is_deleted='N'
--iff linkmanUuid != null and linkmanUuid != ''
    AND linkman_uuid='#linkmanUuid#'
--iff linkmanUuids != null
    AND linkman_uuid IN
    -- for item=item index=index collection=linkmanUuids open=( separator=, close=)
        '#item#'
    -- end
--iff subcUuid != null and subcUuid != ''
    AND subc_uuid = '#subcUuid#'
--iff subcUuids != null
    AND subc_uuid IN
    -- for item=item index=index collection=subcUuids open=( separator=, close=)
        '#item#'
    -- end
--end

