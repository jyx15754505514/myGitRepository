-- [listSubLinkman]
SELECT
	linkman_uuid linkmanUuid,
	subc_uuid subcUuid,
	linkman_name linkmanName,
	gender gender,
	phone phone,
	email email,
	fax fax,
	office_phone officePhone,
	addr addr,
	is_default isDefault,
	status_uuid statusUuid,
	dept dept,
	position position,
	role role,
	qq qq,
	we_chat weChat,
	type_of_service typeOfService,
	comments comments,
	is_deleted isDeleted,
	created_time createdTime,
	created_uuid createdUuid,
	created_name createdName,
	updated_time updatedTime,
	updated_name updatedName,
	updated_uuid updatedUuid,
	extend1,
	extend2,
	extend3
FROM
	crm_subc_linkman
WHERE
	is_deleted = 'N'
    --iff linkmanName !=null and linkmanName != ''
        and linkman_name = '#linkmanName:like#'
    --end
    --iff linkmanUuid !=null and linkmanUuid != ''
        and linkman_uuid = '#linkmanUuid#'
    --end
    --iff subcUuid !=null and subcUuid != ''
        and subc_uuid = '#subcUuid#'
    --end


