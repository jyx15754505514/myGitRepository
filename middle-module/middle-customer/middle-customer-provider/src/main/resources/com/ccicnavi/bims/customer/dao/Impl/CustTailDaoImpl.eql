-- [listCustTail]
SELECT
	tail_uuid,
	cust_uuid,
	person_uuid,
	person_name,
	status_uuid,
	performance,
	visit_type,
	linkman_purpose,
	idea,
	next_link_time,
	comments,
	is_deleted,
	created_time,
	created_uuid,
	created_name,
	updated_time,
	updated_name,
	extend1,
	extend2,
	extend3
FROM
	crm_customer_tail
WHERE
	is_deleted = 'N'
        --if personName !=null and personName != ''
            and person_name = '#personName:like#'
        --end

-- [getCustTail]
SELECT
	tail_uuid,
	cust_uuid,
	person_uuid,
	person_name,
	status_uuid,
	performance,
	visit_type,
	linkman_purpose,
	idea,
	next_link_time,
	comments,
	is_deleted,
	created_time,
	created_uuid,
	created_name,
	updated_time,
	updated_name,
	extend1,
	extend2,
	extend3
FROM
	crm_customer_tail
WHERE
	is_deleted = 'N'
and tail_uuid = '#tailUuid#'

