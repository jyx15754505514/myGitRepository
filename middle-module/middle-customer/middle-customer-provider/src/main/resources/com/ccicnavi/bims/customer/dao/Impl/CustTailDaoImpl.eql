-- [deleteCustTail]
UPDATE crm_customer_tail
SET is_deleted = 'Y'
WHERE
	is_deleted = 'N'
     -- if uuids != null
            AND tail_uuid IN
            -- for item=item index=index collection=uuids open=( separator=, close=)
                #item#
            -- end
        -- end

-- [updateCustTail]
 update crm_customer_tail
