      -- iff linkmanPurpose != null
        linkman_purpose = '#linkmanPurpose#',

      -- iff idea != null
        idea = '#idea#',

      -- iff nextLinkTime != null and nextLinkTime!= ''
        next_link_time = '#nextLinkTime#',

      -- iff comments != null
        comments = '#comments#',

      -- iff isDeleted != null
        is_deleted = '#isDeleted#',

      -- iff createdTime != null
