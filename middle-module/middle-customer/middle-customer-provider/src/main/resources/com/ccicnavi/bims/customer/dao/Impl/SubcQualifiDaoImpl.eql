-- [listSubcuQuali]
SELECT
    subc_qualifi_uuid subcQualifiUuid,
    subc_uuid subcUuid,
    qualifi_range qualifiRange,
    qualifi_enm_uuid qualifiEnmUuid,
    qualifi_code qualifiCode,
    qualifi_describe qualifiDescribe,
    is_file isFile,
    is_deleted isDeleted,
    comments comments,
    created_time createdTime,
    created_uuid createdUuid,
    updated_time updatedTime,
    updated_uuid updatedUuid,
    updated_name updatedName,
    extend1 extend1,
    extend2 extend2,
    extend3 extend3
FROM
    crm_subc_qualification
WHERE
    is_deleted='N'

