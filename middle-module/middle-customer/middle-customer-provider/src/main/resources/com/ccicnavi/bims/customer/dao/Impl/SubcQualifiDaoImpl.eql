       -- iff qualifiEnmUuid != null
         qualifi_enm_uuid = '#qualifiEnmUuid#',
       -- iff qualifiCode != null
         qualifi_code = '#qualifiCode#',
      -- iff qualifiDescribe != null
        qualifi_describe = '#qualifiDescribe#',
      -- iff isFile != null
        is_file = '#isFile#',
      -- iff comments != null
        comments = '#comments#',
      -- iff isDeleted != null
        is_deleted = '#isDeleted#',
      -- iff createdTime != null
        created_time = '#createdTime#',
      -- iff createdUuid != null
        created_uuid = '#createdUuid#',
     -- iff createdName != null
        created_name = '#createdName#',
      -- iff updatedTime != null
