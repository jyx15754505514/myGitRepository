-- [updateSubcuQuali]
 update crm_subc_qualification
	-- trim prefix=SET suffixOverrides=,
      -- iff subcQualifiUuid != null
        subc_qualifi_uuid = '#subcQualifiUuid#',

      -- iff subcUuid != null
