-- [listCatalogDO]
SELECT
    prod_catalog_uuid,
    catalog_name,
    catalog_code,
    comments,
    is_deleted,
    created_time,
    created_uuid,
    created_name,
    updated_time,
    updated_uuid,
    updated_name,
    org_uuid,
    app_sys_uuid,
    extend1,
    extend2,
    extend3
FROM
    prod_catalog
WHERE
    is_deleted='N'

