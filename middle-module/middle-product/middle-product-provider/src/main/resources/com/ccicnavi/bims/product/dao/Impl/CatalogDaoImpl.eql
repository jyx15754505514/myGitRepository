-- [removeCatalogDO]
UPDATE prod_catalog
SET is_deleted='Y'
WHERE is_deleted='N'
AND prod_catalog_uuid='#prodCatalogUuid#'




-- [updateCatalogDO]
 update prod_catalog
	-- trim prefix=SET suffixOverrides=,
      -- iff prodCatalogUuid != null
        prod_catalog_uuid = '#prodCatalogUuid#',

      -- iff catalogName != null
        catalog_name = '#catalogName#',

      -- iff catalogCode != null
        catalog_code = '#catalogCode#',

      -- iff comments != null
        comments = '#comments#',

      -- iff isDeleted != null
