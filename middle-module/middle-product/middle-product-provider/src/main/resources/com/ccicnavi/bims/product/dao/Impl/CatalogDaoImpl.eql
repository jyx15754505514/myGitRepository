        --end

-- [getCatalogThroughOrgnUUid]
SELECT
	pc.prod_catalog_uuid
FROM
	prod_catalog_org pco
	LEFT JOIN prod_catalog pc ON pco.prod_catalog_uuid = pc.prod_catalog_uuid
WHERE
	pc.is_deleted = 'N'
	    --iff orgUUid !=null and orgUuid !=''
	        AND pco.organization_uuid = 'CCICXN'
	    --end
