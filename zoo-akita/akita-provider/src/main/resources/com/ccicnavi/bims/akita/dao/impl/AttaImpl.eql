-- [listAttaTmp]
SELECT
	t2.id,
	t2.file_new_name,
	t2.file_old_name,
	t2.file_ext,
	t2.file_type,
	t2.file_group,
	t2.create_by,
	t2.remark,
	t1.busin_id
FROM
	sys_atta_tmp t1,
	sys_atta t2
WHERE
	t1.atta_id = t2.id
	AND t1.busin_id in
    -- for item=item index=index collection=list open=( separator=, close=)
    #item#
    -- end


--[deleteAtta]
delete from sys_atta
 where id = '##'


--[deleteAttaTmp]
delete from sys_atta_tmp
where atta_id = '##'


